{
  "version": 2,
  "projects": {
    "shell-ui": {
      "root": "apps/shell-ui",
      "sourceRoot": "apps/shell-ui/app",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nx/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/shell-ui"
          }
        },
        "serve": {
          "executor": "@nx/next:server",
          "options": {
            "dev": true,
            "port": 3000
          }
        }
      }
    },
    "accounting": {
      "root": "apps/accounting",
